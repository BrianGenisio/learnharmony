{
    "env": {
        "browser": true
    },
    "rules": {
        "curly": 2,
        "no-console": 2,
        "no-debugger": 2,
        "no-alert": 2,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "no-multi-spaces": 0,
        "no-unused-vars": 1,
        "semi": 1,
        "global-strict": 0,
        "wrap-iife": [2, "outside"],
        "quotes": [2, "double"],
        "key-spacing": 0,
        "no-trailing-spaces": 0,
        "eol-last": 2
    }
}
